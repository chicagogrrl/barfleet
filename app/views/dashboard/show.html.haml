%section.jumbotron.text-center
  %h1
    Welcome, #{@user[:info][:name]}
%section.container
  .panel.panel-default
    .panel-heading Normalized User Profile
    .panel-body
      %pre= JSON.pretty_generate(@user[:info])
  .panel.panel-default
    .panel-heading Full User Profile
    .panel-body
      %pre= JSON.pretty_generate(@user[:extra][:raw_info])
